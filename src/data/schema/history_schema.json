{
    "history" : {
        "type" : "array",
        "uniqueItems": true,
        "project" :{
            "type" : "object",
            "properties" : {
                "projId" : {
                    "type" : "integer",
                    "minimum" : 0
                },
                "lastRunDate" : {
                    "type" : "string",
                    "pattern" : "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                }
            },
            "required": [ 
                "projId", 
                "runDate"
            ]
        }
    }
}